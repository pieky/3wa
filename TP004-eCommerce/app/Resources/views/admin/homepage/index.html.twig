{% extends 'base.html.twig' %}

{% block body %}

   <div class="row">
      <div class="col-xs-9 col-xs-offset-1">
         <div class="page-header">
            <h1>Admin <small> {{ 'admin.user.title' | trans() }}</small></h1>
         </div>

         <!-- Nav tabs -->
         <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active"><a href="#home" aria-controls="home" role="tab" data-toggle="tab">User List</a></li>
            <li role="presentation"><a href="#profile" aria-controls="profile" role="tab" data-toggle="tab">User Token</a></li>
         </ul>

         <!-- Tab panes -->
         <div class="tab-content">
            <div role="tabpanel" class="tab-pane active" id="home">
               {% include 'admin/inc/user/users.list.html.twig' %}
            </div>
            <div role="tabpanel" class="tab-pane" id="profile">
                {% include 'admin/inc/user/users.tokens.list.html.twig' %}
            </div>
         </div>

      </div>
   </div>

{% endblock %}