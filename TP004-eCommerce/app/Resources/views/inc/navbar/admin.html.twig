{% if is_granted('ROLE_ADMIN') %}
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
            <i class="fa fa-unlock" aria-hidden="true"></i>
            <span class="caret"></span>
        </a>
        <ul class="dropdown-menu">
            <li>
                <a href="{{ url('app.admin.homepage.index') }}">
                    <i class="fa fa-group" aria-hidden="true"></i>
                    {{ 'admin.user.navTitle' | trans() | capitalize }}
                </a>
            </li>
            <li>
                <a href="{{ url('app.admin.category.index') }}">
                    <i class="fa fa-folder" aria-hidden="true"></i>
                    {{ 'admin.category.navTitle' | trans() | capitalize }}
                </a>
            </li>
            <li>
                <a href="{{ url('app.admin.subcategory.index') }}">
                    <i class="fa fa-folder" aria-hidden="true"></i>
                    {{ 'admin.subcategory.navTitle' | trans() | capitalize }}
                </a>
            </li>
        </ul>
    </li>
{% endif %}